<IfModule mod_rewrite.c>
  RewriteEngine On  

  RewriteCond %{SERVER_PORT} 80
  RewriteRule ^(.*)$ https://www.psykologie.com/ [R=301,L]

  RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} -f [OR]
  RewriteCond %{DOCUMENT_ROOT}%{REQUEST_URI} -d
  RewriteRule ^ - [L]

  RewriteRule ^ /index.html [L]
</IfModule>